{
  "name": "@faros-fde-sandbox/cli",
  "version": "1.0.0",
  "description": "Modern CLI for Faros AI - sync data, manage sources, view logs",
  "keywords": [
    "faros",
    "observability",
    "devops",
    "ci-cd",
    "quality",
    "testing",
    "linear",
    "github"
  ],
  "homepage": "https://www.faros.ai",
  "author": "Faros AI, Inc.",
  "license": "Apache-2.0",
  "bin": {
    "faros": "./bin/faros"
  },
  "files": [
    "lib/",
    "bin/"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf lib node_modules",
    "dev": "tsc -p tsconfig.json --watch",
    "fix": "prettier --write 'src/**/*.ts' && npm run lint -- --fix",
    "lint": "eslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "axios": "^1.8.2",
    "axios-retry": "^4.5.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "ci-info": "^4.0.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.4.5",
    "figures": "^6.1.0",
    "ora": "^8.1.0",
    "p-limit": "^3.1.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.2.2",
    "test-results-parser": "^0.2.3",
    "update-notifier": "^7.3.1",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.17",
    "@types/update-notifier": "^6.0.8",
    "js-yaml": "^4.1.1",
    "prettier": "^2.7.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.6"
  }
}
